{
    "name": "cse335",
    "version": "0.0.1",
    "description": "CSE335 Website",
    "private": true,
    "workspaces": [
        "vendor\/cl\/*",
        "packages\/*"
    ],
    "scripts": {
        "build:dev": "webpack --config webpack.dev.js",
        "build:prod": "webpack --config webpack.prod.js",
        "build:all": "$npm_execpath run build:dev && $npm_execpath run build:prod",
        "profile:dev": "webpack --config webpack.dev.js --profile --json > stats.json",
        "profile:prod": "webpack --config webpack.prod.js --json > stats.json",
        "test": "karma start karma.conf.js",
        "serve": "webpack-dev-server  --config webpack.serve.js --open",
        "build:docs:course": "cd vendor\/cl\/course && npm run build:docs",
        "build:docs:review": "cd vendor\/cl\/review && npm run build:docs",
        "build:docs:site": "cd vendor\/cl\/site && npm run build:docs",
        "build:docs:interact": "cd vendor\/cl\/interact && npm run build:docs",
        "build:docs": "yarn run build:docs:site && npm run build:docs:course && npm run build:docs:interact",
        "cl-publish": "yarn node vendor\/cl\/site\/publisher.js",
        "build:docs:grades": "cd vendor\/cl\/grades && npm run build:docs",
        "build:docs:console": "cd vendor\/cl\/console && npm run build:docs"
    },
    "keywords": [
        "Education",
        "Course Websites"
    ],
    "author": "Charles B. Owen",
    "license": "MIT",
    "dependencies": {
        "console-cl": "~0.0.1",
        "course-cl": "~0.0.1",
        "examples-cl": "~0.0.1",
        "filesystem-cl": "~0.0.1",
        "grades-cl": "~0.0.1",
        "interact-cl": "~0.0.1",
        "quiz-cl": "~0.0.1",
        "review-cl": "~0.0.1",
        "scheduler-cl": "~0.0.1",
        "site-cl": "^0.0.1",
        "step-cl": "~0.0.1",
        "survey-cl": "~0.0.1",
        "team-cl": "~0.0.1",
        "trello-cl": "~0.0.1",
        "users-cl": "~0.0.1"
    },
    "devDependencies": {
        "@babel\/core": "^7.22.9",
        "@babel\/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel\/plugin-transform-runtime": "^7.22.9",
        "@babel\/polyfill": "^7.12.1",
        "@babel\/preset-env": "^7.22.9",
        "@babel\/runtime": "7.22.6",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "11.0.0",
        "cpx": "^1.5.0",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "filemanager-webpack-plugin": "^8.0.0",
        "fs-extra": "^11.1.1",
        "jasmine": "^5.0.2",
        "karma": "^6.4.2",
        "karma-chrome-launcher": "^3.2.0",
        "karma-jasmine": "^5.1.0",
        "karma-sourcemap-loader": "^0.4.0",
        "karma-webpack": "^5.0.0",
        "make-dir": "^4.0.0",
        "resolve-url-loader": "^5.0.0",
        "rimraf": "^5.0.1",
        "sass": "^1.63.6",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "terser-webpack-plugin": "^5.3.9",
        "url-loader": "^4.1.1",
        "vue": "^3.3.4",
        "vue-loader": "^17.2.2",
        "vue-template-compiler": "^2.7.14",
        "webpack": "^5.88.1",
        "webpack-chunk-renamer-plugin": "^1.0.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "4.15.1",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-merge": "^5.9.0"
    },
    "packageManager": "yarn@3.3.1"
}